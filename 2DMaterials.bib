@Article{DresselhausJorioSouzaSaito2010,
  Title                    = {Defect characterization in graphene and carbon nanotubes using {Raman} spectroscopy},
  Author                   = {Dresselhaus, M. S. and Jorio, A. and Souza Filho, A. G. and Saito, R.},
  Journal                  = {Phil. Trans. R. Soc. A},
  Year                     = {2010},
  Number                   = {1932},
